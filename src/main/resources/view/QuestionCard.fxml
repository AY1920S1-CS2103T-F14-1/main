                    <Font name="Arial Rounded MT Bold" size="14.0" />
                </font>
            </Label>
            <Label fx:id="title" text="Two Sum">
                <font>
                    <Font name="Arial Rounded MT Bold" size="14.0" />
                </font>
            </Label>

        </HBox>
        <FlowPane fx:id="tags" hgap="5.0" prefHeight="15.0" prefWidth="203.0" />
        <VBox prefWidth="203.0" spacing="2.0">
            <HBox spacing="3.0">

                <Label text="Difficulty:">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="12.0" />
                    </font>
                </Label>
                <Label fx:id="difficulty" text="Test Difficulty">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="12.0" />
                    </font>
                </Label>

            </HBox>
            <HBox spacing="3.0">

                <Label text="Status:">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="12.0" />
                    </font>
                </Label>
                <Label fx:id="status" text="Some Status">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="12.0" />
                    </font>
                </Label>

            </HBox>

        </VBox>

    </VBox>
</AnchorPane>
