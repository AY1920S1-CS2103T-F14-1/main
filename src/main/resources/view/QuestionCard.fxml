<AnchorPane fx:id="cardPane" prefHeight="93.0" prefWidth="197.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">

    <VBox layoutX="5.0" layoutY="18.0" prefHeight="76.0" prefWidth="187.0" spacing="5.0" AnchorPane.bottomAnchor="7.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="7.0">

        <HBox spacing="5.0">
            <Label fx:id="id" text="[1]">
