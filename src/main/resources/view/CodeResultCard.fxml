<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane fx:id="cardPane" prefHeight="235.0" prefWidth="411.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="5.0" layoutY="4.0" prefHeight="125.0" prefWidth="395.0" spacing="2.0">
         <children>
            <HBox spacing="5.0">
               <children>
                  <Label text="Test Case">
                     <font>
                        <Font name="Century Gothic" size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="testCaseId" text="6">
                     <font>
                        <Font name="Century Gothic" size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <FlowPane fx:id="tags" hgap="5.0" prefHeight="15.0" prefWidth="203.0" />
            <VBox prefHeight="89.0" prefWidth="464.0" spacing="2.0" translateY="20.0">
               <children>
                  <HBox spacing="3.0">
                     <children>
                        <Label text="Input:">
                           <font>
                              <Font name="Century Gothic" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="input" prefHeight="18.0" prefWidth="306.0" text="3">
                           <font>
                              <Font name="Century Gothic" size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox prefHeight="15.0" prefWidth="486.0" spacing="3.0">
                     <children>
                        <Label>
                           <font>
                              <Font name="Arial Rounded MT Bold" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox prefHeight="17.0" prefWidth="526.0" spacing="3.0">
                     <children>
                        <Label text="Expected Output:">
                           <font>
                              <Font name="Century Gothic" size="13.0" />
                           </font>
                        </Label>
                        <Label fx:id="expectedOutput" prefHeight="18.0" prefWidth="255.0" text="Fizz">
                           <font>
                              <Font name="Century Gothic" size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="10.0" spacing="3.0">
                     <children>
                        <Label fx:id="input1" text=" ">
                           <font>
                              <Font name="Arial Rounded MT Bold" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="61.0" prefHeight="66.0" prefWidth="395.0" spacing="3.0">
                     <children>
                        <Label text="Your Output:">
                           <font>
                              <Font name="Century Gothic" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="actualOutput" prefHeight="18.0" prefWidth="280.0" text="FizzBuzz">
                           <font>
                              <Font name="Century Gothic" size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
      <TextFlow layoutX="6.0" layoutY="178.0" prefHeight="20.0" prefWidth="395.0">
         <children>
            <Label fx:id="evaluation" text="FAILED TEST CASE">
               <font>
                  <Font name="Century Gothic" size="14.0" />
               </font>
            </Label>
         </children>
      </TextFlow>
      <TextFlow layoutX="6.0" layoutY="203.0" prefHeight="20.0" prefWidth="395.0">
         <children>
            <Label fx:id="failReason" text="INCORRECT OUTPUT">
               <font>
                  <Font name="Century Gothic" size="14.0" />
               </font>
            </Label>
         </children>
      </TextFlow>
   </children>
   <padding>
      <Insets bottom="10.0" right="10.0" />
   </padding>
</AnchorPane>
